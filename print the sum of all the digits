import java.util.*;
class Main{
    public static void main(String[]args){
        Scanner obj=new Scanner(System.in);
        System.out.print("Input:");
        int num=obj.nextInt();
        int sum=0;
        for(int i=num;i>0;i=i/10){
            int digit = i % 10;
            sum =sum+digit;
        }
        System.out.println("Output: " + sum);
    }
}
