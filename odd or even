import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner obj=new Scanner(System.in);
        {
            System.out.print("Enter the number:");
         int num=obj.nextInt();
         if(num%2==0){
             System.out.println("Given Number is Even");
         }
         else
         {
              System.out.println("Given Number is Odd");
         }
}}}
